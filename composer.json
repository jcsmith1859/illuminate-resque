{
  "name": "jcsmith1859/illuminate-resque",
  "description": "Illuminate Resque connector for the Laravel Queue class.",
  "keywords": ["illuminate", "laravel", "resque", "queue"],
  "license": "MIT",
  "authors": [
    {
      "name": "Jared Smith",
      "email": "jcsmith1859@gmail.com"
    }
